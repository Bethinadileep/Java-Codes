//Write a Program for call by value in Java
import java.util.*;
public class Main
{
    int marks = 99;
       //call by value
        void subject(int marks){
            marks = marks-16;
            System.out.println(marks);
        }
	public static void main(String[] args) {
	    Main obj = new Main();
	    System.out.println("Before Executing the marks is"+obj.marks);
	    obj.subject(88);
	    System.out.println("After Executing the marks is"+obj.marks);
	
	    
	}
}


