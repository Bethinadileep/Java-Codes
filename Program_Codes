//Write a program to sort the array in Java
import java.util.Arrays;
public class Main
{
	public static void main(String[] args) {
	    int[] arr = {23,1,34,20,16,32,9,5,6,2,0};
	    Arrays.sort(arr);
	    for(int i=0;i<=arr.length-1;i++) {
	        System.out.print(arr[i]+" ");
	    }
  }
}

